{
  "name": "json-splora",
  "version": "0.0.1",
  "description": "Data exploration and sharing",
  "main": "index.js",
  "bin": {
    "jsplora": "bin/jsplora"
  },
  "scripts": {
    "start": "ENV=development electron .",
    "build-darwin": "node_modules/.bin/electron-packager . --darwin --overwrite",
    "build-linux": "node_modules/.bin/electron-packager . --linux --overwrite",
    "build-win32": "node_modules/.bin/electron-packager . --win32 --overwrite",
    "build": "npm run-script build-darwin && npm run-script build-linux && npm run-script build-win32"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Wells Johnston",
  "devDependencies": {
    "electron-packager": "^8.3.0"
  },
  "dependencies": {
    "electron": "^1.4.8",
    "codemirror": "^5.20.2",
    "electron-context-menu": "^0.8.0",
    "electron-default-menu": "^1.0.0",
    "jquery": "^3.1.1",
    "json5": "^0.5.0",
    "jsonlint": "^1.6.2",
    "menu": "^0.2.5",
    "node-jq": "^0.4.0"
  }
}
